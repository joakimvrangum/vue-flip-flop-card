{
  "name": "vue-flip-flop-card",
  "description": "The is a card.. for VueJS.. that flips!",
  "keywords": [
    "vue",
    "vuejs",
    "component",
    "javascript",
    "card",
    "flip card"
  ],
  "version": "0.2.5",
  "main": "dist/vue-flip-flop-card.ssr.js",
  "module": "dist/vue-flip-flop-card.esm.js",
  "unpkg": "dist/vue-flip-flop-card.min.js",
  "browser": {
    "./sfc": "src/flip-flop-card.vue"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joakimvrangum/vue-flip-flop-card.git"
  },
  "author": "Joakim Vrangum",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joakimvrangum/vue-flip-flop-card/issues"
  },
  "homepage": "https://github.com/joakimvrangum/vue-flip-flop-card#readme",
  "scripts": {
    "serve": "vue-cli-service serve ./dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config ./build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config ./build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config ./build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config ./build/rollup.config.js --format iife"
  },
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.57.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass": "^1.42.1",
    "sass-loader": "^10.2.0",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  }
}
